    "next": "14.2.3",
    "react": "18.3.1",
    "react-aria-components": "1.2.0",
    "react-dom": "18.3.1",
    "react-i18next": "14.1.1",
    "react-select": "5.8.0",
    "styled-components": "6.1.9",
    "zustand": "4.5.2"
