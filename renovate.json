{
  "extends": [
    "github>numerique-gouv/renovate-configuration"
  ],
  "dependencyDashboard": true,
  "packageRules": [
    {
      "enabled": false,
      "groupName": "ignored python dependencies",
      "matchManagers": [
        "pep621"
      ],
      "matchPackageNames": []
    },
    {
      "enabled": false,
      "groupName": "ignored js dependencies",
      "matchManagers": [
        "npm"
      ],
      "matchPackageNames": [
        "node",
        "node-fetch"
      ]
    }
  ]
}
